# Enable Rewrite Engine
RewriteEngine On

# Force HTTPS
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]

# Force non-www
RewriteCond %{HTTP_HOST} ^www\.greenlineexpress\.in$ [NC]
RewriteRule ^(.*)$ https://greenlineexpress.in/$1 [L,R=301]

# Remove index.php
RewriteCond %{THE_REQUEST} \s/index\.php [NC]
Rewrit
